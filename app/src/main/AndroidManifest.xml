<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.a36432.superalarmclock">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SetActivity" />
        <activity android:name=".NoteActivity" />
        <activity android:name=".NoteEditActivity" />
        <activity android:name=".ExploreActivity" />
        <activity android:name=".NoteItemActivity" />
        <activity android:name=".SetMusicActivity" />
        <activity android:name=".SetMusicFavoritesActivity" />
        <activity android:name=".SetBackActivity" />
        <activity android:name=".WeatherActivity" />
        <activity android:name=".WakeupActivity"></activity>
    </application>

</manifest>